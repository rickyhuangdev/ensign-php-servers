{
    "name": "rickyhuang/package",
    "description": "A very simple library for our ensign project",
    "keywords": [
        "rickyhuang",
        "ensign",
        "services"
    ],
    "autoload": {
        "psr-4": {
            "Rickytech\\Library\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rickytech\\Library\\Tests\\": "tests"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "Ricky Huang",
            "email": "ricky_huang_hkg@ensignfreight.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/http-foundation": "^6.1",
        "hyperf/exception-handler": "~3.0.0",
        "hyperf/paginator": "~3.0.0",
        "hyperf/elasticsearch": "~3.0.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "v3.10.0",
        "phpunit/phpunit": "^9.4"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    }
}
