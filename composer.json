{
    "name": "rickyhuang/package",
    "description": "A very simple library for our ensign project",
    "keywords": [
        "rickyhuang",
        "ensign",
        "services"
    ],
    "autoload": {
        "psr-4": {
            "Rickytech\\Library\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Rickytech\\Library\\Tests\\": "tests"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "Ricky Huang",
            "email": "ricky_huang_hkg@ensignfreight.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/http-foundation": "^6.2.x-dev",
        "hyperf/exception-handler": "~3.0.0",
        "hyperf/paginator": "~3.0.0",
        "hyperf/elasticsearch": "~3.0.0",
        "robinvdvleuten/ulid": "^5.0",
        "godruoyi/php-snowflake": "^2.1",
        "hyperf/db-connection": "^3.0.x-dev",
        "hyperf/snowflake": "^3.0",
        "illuminate/support": "^9.41",
        "hyperf/redis": "3.0.*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "friendsofphp/php-cs-fixer": "v3.10.0",
        "phpunit/phpunit": "^9.4"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    }
}
